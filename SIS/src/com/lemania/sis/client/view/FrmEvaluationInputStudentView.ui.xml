<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:p1="urn:import:com.google.gwt.user.cellview.client">

	<g:HTMLPanel width="100%">
		<g:VerticalPanel width="100%" spacing="2" ui:field="pnlEvaluationMain">
			<g:Label text="EVALUATION INTERMEDIAIRE" horizontalAlignment="ALIGN_CENTER" styleName="pageTitle"/>
			<g:HorizontalPanel width="100%">
				<g:Grid cellPadding="2" cellSpacing="3">
				<g:row>
					<g:customCell>
						<g:Label text="1" styleName="step"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="Année :"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox styleName="sis-combobox" ui:field="lstYears"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="4" styleName="step"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="Classes :"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox styleName="sis-combobox" ui:field="lstClasses"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="2" styleName="step"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="Ecole :"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox styleName="sis-combobox" ui:field="lstEcoles"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="5" styleName="step"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="Elèves :"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox styleName="sis-combobox" ui:field="lstBulletins"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="3" styleName="step"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="Programmes :"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox styleName="sis-combobox" ui:field="lstProgrammes"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="6" styleName="step"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="Rapport d&apos;Evaluations :"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox styleName="sis-combobox" ui:field="lstEvaluationHeaders"/>
					</g:customCell>
				</g:row>
				<g:row/>
			</g:Grid>
			</g:HorizontalPanel>
			<g:VerticalPanel width="100%" ui:field="pnlEvaluationPrint">
				<g:VerticalPanel width="100%" horizontalAlignment="ALIGN_CENTER">
				<g:HorizontalPanel width="100%">
					<g:Image height="" url="images/logo_lemania.png" width=""/>
					<g:VerticalPanel width="100%" horizontalAlignment="ALIGN_CENTER">
						<g:Label text="EVALUATION INTERMEDIAIRE" styleName="bulletinTitle"/>
						<g:Grid>
							<g:row>
								<g:customCell>
									<g:Label text="DU :"/>
								</g:customCell>
								<g:customCell>
									<g:Label text="New Label"/>
								</g:customCell>
								<g:customCell>
									<g:Label text="AU :"/>
								</g:customCell>
								<g:customCell>
									<g:Label text="New Label"/>
								</g:customCell>
							</g:row>
						</g:Grid>
						<g:Grid>
										<g:row>
											<g:customCell>
												<g:Label text="Classe :"/>
											</g:customCell>
											<g:customCell>
												<g:Label text="New Label"/>
											</g:customCell>
											<g:customCell>
												<g:Label text="Maître de classe :"/>
											</g:customCell>
											<g:customCell>
												<g:Label text="New Label"/>
											</g:customCell>
										</g:row>
									</g:Grid>
					</g:VerticalPanel>
				</g:HorizontalPanel>
				<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_LEFT">
					<g:Grid>
						<g:row>
							<g:customCell>
								<g:Label text="Objectif spécifique du semestre :"/>
							</g:customCell>
							<g:customCell>
								<g:Label text="New Label" width="300px" height="100px"/>
							</g:customCell>
						</g:row>
					</g:Grid>
					<g:VerticalPanel spacing="3" styleName="subSection">
			<g:Label text="TB = Très Bien"/>
			<g:Label text="B = Bien"/>
			<g:Label text="AB = Assez Bien"/>
			<g:Label text="S = Suffisant"/>
			<g:Label text="NA = Non atteint"/>
		</g:VerticalPanel>
				</g:HorizontalPanel>
				<g:Label text="Objectifs d&apos;apprentissage pour l&apos;élève" styleName="bulletinTitle"/>
			</g:VerticalPanel>
				<g:VerticalPanel width="100%" spacing="2">
				<p1:DataGrid width="100%" height="200px" ui:field="tblEvaluations" styleName="subSection"/>
				<g:HorizontalPanel width="100%" spacing="5">
				<g:Label text="Commentaire(s) éventuel(s) et pistes d&apos;amélioration :"/>
				<g:TextArea visibleLines="5" characterWidth="80" ui:field="txtCommentaire"/>
				<g:Button ui:field="cmdSave" enabled="false">Enregister</g:Button>
			</g:HorizontalPanel>
			</g:VerticalPanel>
			</g:VerticalPanel>
			
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>
