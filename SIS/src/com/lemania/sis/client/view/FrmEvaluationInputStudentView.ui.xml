<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:p1="urn:import:com.google.gwt.user.cellview.client">

	<g:HTMLPanel width="100%">
		<g:VerticalPanel width="100%" spacing="2">
			<g:Label text="CONTACT" horizontalAlignment="ALIGN_CENTER" styleName="pageTitle"/>
			<g:Grid cellPadding="2" cellSpacing="3">
				<g:row>
					<g:customCell>
						<g:Label text="Année :"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox styleName="sis-combobox"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Programmes :"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox styleName="sis-combobox"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Classes :"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox styleName="sis-combobox"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Elèves :"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox styleName="sis-combobox"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Evaluations :"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox styleName="sis-combobox"/>
					</g:customCell>
				</g:row>
			</g:Grid>
			<p1:DataGrid width="100%" height="200px" ui:field="dataGrid"/>
			<g:HorizontalPanel width="100%" spacing="5">
				<g:Label text="Commentaire(s) éventuel(s) et pistes d&apos;amélioration :"/>
				<g:TextArea visibleLines="5" characterWidth="80"/>
				<g:Button>Enregister</g:Button>
			</g:HorizontalPanel>
			
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>
