<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:FlowPanel width="100%" height="100%">
		<g:DockPanel width="100%" height="100%" ui:field="dockPanel" spacing="5">
			<g:Dock direction="NORTH">
				<g:HorizontalPanel verticalAlignment="ALIGN_BOTTOM" width="100%" height="54px" borderWidth="0">
					<g:Image url="images/logo.png" width=""/>
					<g:Grid height="" width="100%" borderWidth="0">
						<g:row>
							<g:customCell>
								<g:Label width="" horizontalAlignment="ALIGN_LEFT" ui:field="txtWelcome"/>
							</g:customCell>
							<g:customCell>
								<g:Hyperlink targetHistoryToken="newHistoryToken" width="" ui:field="cmdLogout"/>
							</g:customCell>
							<g:customCell>
								<g:Label ui:field="lblCurrentMonth"/>
							</g:customCell>
							<g:customCell>
								<g:Button text="Cacher/ Afficher le menu" ui:field="cmdMenuToggle" width=""/>
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:HorizontalPanel>
			</g:Dock>
			<g:Dock direction="WEST">
				<g:VerticalPanel ui:field="leftPanel">
					<g:Tree animationEnabled="true" styleName="sideBar" height="" width="250px" ui:field="treeMenuAdmin">
					<g:Label text="." width="" horizontalAlignment="ALIGN_LEFT" styleName="left_header"/>
					<g:Hyperlink text="Accueil" targetHistoryToken="!homepage"/>
					<g:TreeItem text="Gestion générale des bulletins" state="true">
							<g:Hyperlink text="Programmes / Classes" targetHistoryToken="profilemgt" ui:field="cmdProfileManagement"/>
							<g:Hyperlink text="Attribution Programme / Elève" targetHistoryToken="bulletincreation"/>
							<g:Hyperlink text="Attribution personnalisée" targetHistoryToken="bulletinmanagement"/>
						</g:TreeItem>
					<g:TreeItem text="Notes" state="true">
						<g:Hyperlink text="Saisir les notes" targetHistoryToken="markinput"/>
						<g:Hyperlink text="Consulter les notes" targetHistoryToken="bulletindetail"/>
					</g:TreeItem>
					<g:TreeItem text="Information de base" state="true">
						<g:TreeItem text="Elèves">
							<g:Hyperlink targetHistoryToken="students" text="Liste des élèves"/>
							<g:Hyperlink targetHistoryToken="studentadd" text="Rajouter un élève"/>
						</g:TreeItem>
						<g:TreeItem text="Professeurs">
							<g:Hyperlink targetHistoryToken="!profs" text="Liste des professeurs"/>
							<g:Hyperlink targetHistoryToken="!profsadd" text="Rajouter un professeur"/>
						</g:TreeItem>
						<g:TreeItem text="Branches">
						<g:Hyperlink targetHistoryToken="branchelist" text="Liste des branches"/>
						<g:Hyperlink targetHistoryToken="brancheadd" text="Rajouter une branche"/>
					</g:TreeItem>
						<g:TreeItem text="Matières">
							<g:Hyperlink targetHistoryToken="subjectlist" text="Liste des matières"/>
							<g:Hyperlink targetHistoryToken="subjectadd" text="Rajouter une matière"/>
						</g:TreeItem>
						<g:TreeItem text="Classes">
							<g:Hyperlink text="Liste des classes" targetHistoryToken="classlist"/>
							<g:Hyperlink targetHistoryToken="classeadd" text="Rajouter une classe"/>
						</g:TreeItem>
						<g:TreeItem text="Programmes">
							<g:Hyperlink targetHistoryToken="!cours" text="Liste des programmes"/>
							<g:Hyperlink text="Rajouter un programme" targetHistoryToken="coursadd"/>
						</g:TreeItem>
						<g:TreeItem text="Ecoles">
						<g:Hyperlink text="Liste des écoles" targetHistoryToken="!ecolepage"/>
						<g:Hyperlink targetHistoryToken="!addecole" text="Rajouter une école"/>
						</g:TreeItem>
						<g:TreeItem text="Gestion de l&apos;accès">
							<g:Hyperlink text="Gestion des codes d&apos;accès" targetHistoryToken="usermanagement"/>
							<g:Hyperlink targetHistoryToken="settings" text="Bloquer l&apos;accès au système"/>
						</g:TreeItem>
					</g:TreeItem>
					<g:TreeItem text="Autres">
						<g:Hyperlink targetHistoryToken="password" text="Modifier le mot de passe"/>
						<g:Hyperlink targetHistoryToken="contact" text="Aide / Contact"/>
					</g:TreeItem>
				</g:Tree>
					<g:Tree ui:field="treeMenuProf" styleName="sideBar" width="250px">
						<g:Label styleName="left_header" text="." width=""/>
						<g:TreeItem text="Espace professeur" state="true">
							<g:Hyperlink text="Saisir les notes" targetHistoryToken="markinput"/>
							<g:Hyperlink text="Consulter les bulletins" targetHistoryToken="bulletindetail"/>
							<g:Hyperlink text="Modifier le mot de passe" targetHistoryToken="password"/>
							<g:Hyperlink targetHistoryToken="contact" text="Aide / Contact"/>
						</g:TreeItem>
					</g:Tree>
					<g:Tree styleName="sideBar" ui:field="treeMenuEleve" width="250px">
						<g:Label styleName="left_header" text="." width=""/>
						<g:TreeItem state="true" text="Espace élève">
							<g:Hyperlink targetHistoryToken="bulletindetail" text="Consulter les notes"/>
							<g:Hyperlink targetHistoryToken="password" text="Modifier le mot de passe"/>
							<g:Hyperlink targetHistoryToken="contact" text="Aide / Contact"/>
						</g:TreeItem>
					</g:Tree>
				</g:VerticalPanel>
			</g:Dock>
			<g:Dock direction="CENTER">
				<g:VerticalPanel width="100%" horizontalAlignment="ALIGN_LEFT" height="">
					<g:HTML wordWrap="true" visible="false" ui:field="htmlProgressBar"><div id="fountainG"> <div class="fountainG" id="fountainG_1"> </div> <div class="fountainG" id="fountainG_2"> </div> <div class="fountainG" id="fountainG_3"> </div> <div class="fountainG" id="fountainG_4"> </div> <div class="fountainG" id="fountainG_5"> </div> <div class="fountainG" id="fountainG_6"> </div> <div class="fountainG" id="fountainG_7"> </div> <div class="fountainG" id="fountainG_8"> </div> </div></g:HTML>
					<g:FlowPanel ui:field="mainContentPanel" width="100%" height="" styleName="paneMain"/>
					<g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE" width="100%" horizontalAlignment="ALIGN_RIGHT" spacing="5">
						<g:Label text="Copyright © Ecole Lémania" horizontalAlignment="ALIGN_LEFT"/>
						<g:Image url="images/appengine-silver-120x30.gif"/>
					</g:HorizontalPanel>
				</g:VerticalPanel>
			</g:Dock>
		</g:DockPanel>
	</g:FlowPanel>
</ui:UiBinder>
