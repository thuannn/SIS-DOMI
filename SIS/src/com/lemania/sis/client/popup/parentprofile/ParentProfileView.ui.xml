<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>
    </ui:style>
    <g:DialogBox ui:field="dialog" text="Responsable" width="600px" height="">
    	<g:VerticalPanel width="100%" height="100%" horizontalAlignment="ALIGN_RIGHT">
    		<g:Button styleName="popupCloseButton" ui:field="cmdClose">X</g:Button>
    		<g:HorizontalPanel width="100%" spacing="5">
    			<g:Grid width="100%">
    			<g:row>
    				<g:customCell>
    					<g:Label text="Nom"/>
    				</g:customCell>
    				<g:customCell>
    					<g:TextBox/>
    				</g:customCell>
    			</g:row>
    			<g:row>
    				<g:customCell>
    					<g:Label text="Prénom"/>
    				</g:customCell>
    				<g:customCell>
    					<g:TextBox/>
    				</g:customCell>
    			</g:row>
    			<g:row>
    				<g:customCell>
    					<g:Label text="Email"/>
    				</g:customCell>
    				<g:customCell>
    					<g:TextBox/>
    				</g:customCell>
    			</g:row>
    			<g:row>
    				<g:customCell>
    					<g:Label text="Mobile"/>
    				</g:customCell>
    				<g:customCell>
    					<g:TextBox/>
    				</g:customCell>
    			</g:row>
    			<g:row>
    				<g:customCell>
    					<g:Label text="Téléphone à domicile"/>
    				</g:customCell>
    				<g:customCell>
    					<g:TextBox/>
    				</g:customCell>
    			</g:row>
    			<g:row>
    				<g:customCell>
    					<g:Label text="Téléphone au travail"/>
    				</g:customCell>
    				<g:customCell>
    					<g:TextBox/>
    				</g:customCell>
    			</g:row>
    		</g:Grid>
    			<g:VerticalPanel width="100%" spacing="5">
    				<g:Label text="Accès à l&apos;espace parent des élèves"/>
    				<g:HorizontalPanel>
    					<g:ListBox/>
    					<g:Button>+</g:Button>
    				</g:HorizontalPanel>
    				<g:ListBox visibleItemCount="5" width="250px"/>
    			</g:VerticalPanel>
    		</g:HorizontalPanel>
    		<g:Label text="-" styleName="hiddenText"/>
    		<g:Button text="Valider"/>
    	</g:VerticalPanel>
    </g:DialogBox>
</ui:UiBinder>
